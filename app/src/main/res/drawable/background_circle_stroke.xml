<inset xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        <item
            android:state_pressed="true">
            <shape android:shape="oval">
                <stroke
                    android:width="2dp"
                    android:color="@color/colorAccentLight" />
            </shape>
        </item>
        <item>
            <shape android:shape="oval">
                <stroke
                    android:width="2dp"
                    android:color="@color/colorAccent" />
            </shape>
        </item>
    </selector>
</inset>